package org.sample.streamtutorial;

import java.util.ArrayList;

import org.testng.annotations.Test;




public class Test1 {

	@Test
	public void regular() {

		ArrayList<String> names = new ArrayList<String>();
		names.add("Akash");
		names.add("Damki");
		names.add("pnoop");
		names.add("Aansh");
		names.add("Pune");

		int count = 0;

		for (int i = 0; i < names.size(); i++) {
			String actuals = names.get(i);
			if (actuals.toLowerCase().contains("a")) {
				count++;
			}
		}
		System.out.println(count);
	}
}
